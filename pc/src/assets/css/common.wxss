html,
body,
#app {
  width: 100%;
  height: 100%;
}
* {
  box-sizing: border-box;
}
.page-box {
  position: absolute;
  width: 100%;
  top: 0;
  bottom: 0;
  background-color: #fff;
  overflow-y: auto;
  font-size: 12px;
}
.sigleTextOverflow {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  word-break: break-all;
}
.aline-overflow {
  overflow: hidden;
  white-space: normal;
  word-break: break-all;
}
.main-wrap {
  width: 1200px;
  margin: 0 auto;
}
.table-app-main .main-wrap {
  width: 100%;
  padding: 0px 100px;
}
.form-title {
  width: 100%;
  text-align: center;
  margin: 20px auto;
  font-family: PingFangSC-Medium;
  font-size: 24px;
  color: #333333;
  line-height: 33px;
}
.form-wrap {
  width: 100%;
  padding: 30px;
  padding-top: 50px;
  background: #FFFFFF;
  box-shadow: 0 4px 18px 0 rgba(0, 0, 0, 0.06);
}
.form-wrap /deep/ .el-form-item {
  margin-bottom: 30px;
}
.form-wrap /deep/ .el-form-item__label {
  width: 85px;
  font-size: 14px;
  color: #333333;
}
.form-wrap /deep/ .el-input__inner {
  width: 240px;
}
.form-wrap /deep/ .el-button {
  width: 120px;
  border-radius: 2px;
}
.form-wrap .max-input {
  width: 620px;
}
.form-wrap .max-input /deep/ .el-input__inner {
  width: 100%;
}
.form-wrap .form-subtitle {
  margin-top: 70px;
  margin-bottom: 20px;
  line-height: 35px;
  font-family: PingFangSC-Medium;
  font-size: 18px;
  color: #333333;
}
.form-wrap .form-operation {
  padding-top: 30px;
  display: flex;
  justify-content: center;
}
.search-wrapper {
  padding: 10px 0px;
  background-color: #fff;
}
.search-wrapper /deep/ .el-form-item {
  margin-bottom: 10px;
}
.search-wrapper /deep/ .el-input__inner {
  padding-left: 15px;
}
.search-wrapper /deep/ .el-input {
  width: 170px;
}
.search-wrapper /deep/ .el-select .el-input,
.search-wrapper /deep/ .el-date-editor--datetime {
  width: 240px;
}
.search-wrapper /deep/ .el-input__prefix {
  left: unset;
  right: 5px;
}
.search-wrapper /deep/ .el-button {
  width: 100px;
}
.dynamic-list /deep/ .el-table th {
  background: #F5F5F5;
  font-family: PingFangSC-Medium;
  font-size: 14px;
  color: #333333;
  text-align: center;
  padding: 0px;
  height: 50px;
}
.dynamic-list /deep/ .el-table td {
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #666666;
  text-align: center;
  padding: 0px;
  height: 50px;
}
.dynamic-list /deep/ .el-pagination {
  padding: 0px;
  margin-top: 50px;
  margin-bottom: 80px;
  display: flex;
  justify-content: center;
}
.medium-btn {
  width: 120px;
  height: 40px;
}
.small-btn {
  width: 100px;
  height: 40px;
}
.mini-btn {
  width: 80px;
  height: 40px;
}
.medium-ipt {
  width: 620px;
  height: 40px;
}
.small-ipt {
  width: 240px;
  height: 40px;
}
.smaller-ipt {
  width: 170px;
  height: 40px;
}
.mini-ipt {
  width: 120px;
  height: 40px;
}
